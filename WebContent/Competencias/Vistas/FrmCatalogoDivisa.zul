<?page title="REGISTRO DE DIVISAS" contentType="text/html;charset=UTF-8"?>
<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver" ?>
<?init class="org.zkoss.zkplus.databind.AnnotateDataBinderInit" root="frmCatalogoD"?>
<zk>

	<div align="center">
		<window id="frmCatalogoD" title="Divisas Existentes" apply="controlador.competencia.CntrlFrmCatalogoDivisa"
			border="normal" mode="modal" closable="true" width="420px" height="350px">
			<panel width="100%" border="normal" >
				<panelchildren>
					<div style="margin-top:5px">
					<label value="Filtro"></label>
					<textbox></textbox>
					<button label="Buscar"></button>
					</div>
					<listbox id="lsbxDivisas" mold="paging" pageSize="10" style="margin-top:5px" model="@{cntrl.divisas}" height="240px">
						<listhead>
							<listheader label="Nombre" align="center"
								width="25%">
							</listheader>
							<listheader label="Estado" align="center"
								width="25%">
							</listheader>
							<listheader label="TelÃ©fono" align="center"
								width="25%">
							</listheader>
						</listhead>
						
						<listitem self="@{each=divisa}">
							<listcell label="@{divisa.nombre}"></listcell>
							<listcell label="@{divisa.estadoVenezuela.nombre}"></listcell>
							<listcell label="@{divisa.Telefono}"></listcell>
						</listitem>
									
						
					</listbox>
					<div align="right">
						<button id="btnAceptar"
							image="../../Recursos/Imagenes/aceptar.ico" label="Aceptar">
						</button>
						<button id="btnSalir"
							image="../../Recursos/Imagenes/salir.png" label="Salir"
							onClick="">
						</button>
					</div>
				</panelchildren>
			</panel>


			<zscript><![CDATA[
	public void Salir() {
		FrmCatalogoDivisa.onClose();
	}
]]></zscript>


		</window>
	</div>
</zk>